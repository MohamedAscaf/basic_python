""" 
from tkinter import *
a = Tk()
a.geometry("500x500")

I1 = Label(a,text="Enter the First Value")
I1.grid(row=0,column=1)
I2 = Label(a,text="Enter The Second Value")
I2.grid(row=1,column=1)
I3 = Label(a,text="result")
I3.grid(row=2,column=1)

L1 = Entry(a)
L1.grid(row=0,column=2)
L2 = Entry(a)
L2.grid(row=1,column=2)
L3 = Entry(a)
L3.grid(row=2,column=2)

def add():
    a=int(L1.get())
    b=int(L2.get())
    c=a+b
    L3.insert(0,c)

def sub():
    a=int(L1.get())
    b=int(L2.get())
    c=a-b
    L3.insert(0,c)

def mul():
    a=int(L1.get())
    b=int(L2.get())
    c=a*b
    L3.insert(0,c)

def div():
    a=int(L1.get())
    b=int(L2.get())
    c=a/b
    L3.insert(0,c)      

b=Button(a,text="add" , activebackground="green" , command = lambda:add())                             
b.grid(row=4 , column=1)

b=Button(a,text="sub" , activebackground="blue" , command = lambda:sub())                             
b.grid(row=4 , column=2)

b=Button(a,text="mul" , activebackground="yellow" , command = lambda:mul())                             
b.grid(row=4 , column=3)

b=Button(a,text="div" , activebackground="pink" , command = lambda:div())                             
b.grid(row=4 , column=4)

a.mainloop()

 """



""" from tkinter import *

def onclick():
   pass

root = Tk()
text = Text(root,height=2, width=30)
text.insert(INSERT, "Hello.....")
text.insert(END, "Bye Bye.....")
text.pack()
b=Button(root,text="submit")
b.pack()
text.tag_add("here", "1.0", "1.4")
text.tag_add("start", "1.8", "1.13")
text.tag_config("here", background="orange", foreground="blue")
text.tag_config("start", background="black", foreground="green")
root.mainloop() """



#example
""" from tkinter import *

def onclick():
   pass
root = Tk()
text = Text(root,height=5, width=30)
text.insert(INSERT, "HEllo.....")
text.insert(INSERT, "Bye.....")
text.insert(END, "Bye  ......")

text.pack()
b=Button(root,text="Submit")
b.pack()

text.tag_add("here", "1.0" , "1.5")
text.tag_add("her", "1.18" , "1.21")

text.tag_config("here", background="orange", foreground="blue")
text.tag_config("her", background="green", foreground="blue")
root.mainloop() """



""" from tkinter import *
from tkinter import ttk
root = Tk()
root.title("Combobox Example")
root.geometry('300x300')
combo = ttk.Combobox(root, values=["Option 1", "Option 2", "Option 3", "Option 4", "Option 5"])
combo.pack()

def option_selected(event):
   selected_option = combo.get()
   print("You selected:", selected_option)
combo.bind("<<ComboboxSelected>>", option_selected)
root.mainloop()
 """



#example
""" from tkinter import *
from tkinter import ttk
root = Tk()
root.title("combobox Example")
root.geometry('500x500')
combo = ttk.Combobox(root,  values=["computer 1", "Computer 2", "Computer 3", "Computer 4", "Computer 5"])
combo.pack()
def option_selected(event):
    selected_option  = combo.get()
    print("You selected", selected_option)
combo.bind("<<ComboboxSelected>>", option_selected)
root.mainloop() """




""" from tkinter import *

