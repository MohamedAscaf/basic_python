m2 = Label(a,text="Enter The Second Value")
m2.grid(row=1,column=1)
m3 = Label(a,text="Result")
m3.grid(row=2,column=1)

n1=Entry(a)
n1.grid(row=0,column=2)
n2=Entry(a)
n2.grid(row=1,column=2)
n3=Entry(a)
n3.grid(row=2,column=2)

def sub():
    a=int(n1.get())
    b=int(n2.get())
    c=a-b
    n3.insert(0,c)

b=Button(a,text="sub" , activebackground="blue" , command = lambda:sub())
b.grid(row=3 , column=3)

a.mainloop() """




