""" from tkinter import *
from PIL import ImageTk,Image
from tkinter import ttk 

win=Tk()
win.geometry("500x500")
frame=Frame(win,width=100,height=100)
frame.pack()
frame.place(relx=0.01,rely=0.01)
image=ImageTk.PhotoImage(Image.open("jw1.jpg"))
label=Label(frame,image=image)
label.pack()

frame1=Frame(win,width=100,height=100)
frame1.pack()
frame1.place(relx=0.2,rely=-0.3)
image=ImageTk.PhotoImage(Image.open("jw2.jpg"))
label=Label(frame1,image=image)
label.pack()
image=ImageTk.PhotoImage(Image.open("jw1.jpg"))
label=Label(frame1,image=image)
label.pack()

win.mainloop()
 """




